<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.atlmovie.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.atlmovie.ui.register.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_signUpFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/signUpFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.example.atlmovie.ui.splashscreen.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_onboardingFragment2"
            app:destination="@id/onboardingFragment"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashScreenFragment_to_letsYouInFragment"
            app:destination="@id/letsYouInFragment" />
        <action
            android:id="@+id/action_splashScreenFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/onboardingFragment"
        android:name="com.example.atlmovie.ui.onboarding.OnboardingFragment"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding" >
        <action
            android:id="@+id/action_onboardingFragment_to_letsYouInFragment"
            app:destination="@id/letsYouInFragment"
            app:popUpTo="@id/onboardingFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/letsYouInFragment"
        android:name="com.example.atlmovie.ui.letsyouin.LetsYouInFragment"
        android:label="fragment_lets_you_in"
        tools:layout="@layout/fragment_lets_you_in" >
        <action
            android:id="@+id/action_letsYouInFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_letsYouInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_letsYouInFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.atlmovie.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_seeAllFragment"
            app:destination="@id/seeAllFragment" />
        <action
            android:id="@+id/action_homeFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_exploreFragment"
            app:destination="@id/exploreFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.atlmovie.ui.detail.DetailFragment"
        android:label="DetailFragment" >
        <argument android:name="detail"
            app:argType="integer"/>
    </fragment>
    <fragment
        android:id="@+id/moreLikeThisFragment"
        android:name="com.example.atlmovie.ui.detail.morelikethis.MoreLikeThisFragment"
        android:label="fragment_more_like_this"
        tools:layout="@layout/fragment_more_like_this" />
    <fragment
        android:id="@+id/trailersFragment"
        android:name="com.example.atlmovie.ui.detail.trailers.TrailersFragment"
        android:label="fragment_trailers"
        tools:layout="@layout/fragment_trailers">
        <argument
            android:name="detail"
            app:argType="integer" />
    </fragment>
    <action
        android:id="@+id/action_global_detailFragment"
        app:destination="@id/detailFragment" />
    <fragment
        android:id="@+id/seeAllFragment"
        android:name="com.example.atlmovie.ui.seeall.SeeAllFragment"
        android:label="SeeAllFragment">
        <argument
            android:name="listType"
            app:argType="string" />
        <action
            android:id="@+id/action_seeAllFragment_to_exploreFragment"
            app:destination="@id/exploreFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.example.atlmovie.ui.notifications.NotificationsFragment"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.atlmovie.ui.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_profileFragment_to_notificationSettingsFragment"
            app:destination="@id/notificationSettingsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_profileFragment_to_privacyFragment"
            app:destination="@id/privacyFragment" />
        <action
            android:id="@+id/action_profileFragment_to_helpCenterFragment"
            app:destination="@id/helpCenterFragment" />
        <action
            android:id="@+id/action_profileFragment_to_subscribeToPremiumFragment"
            app:destination="@id/subscribeToPremiumFragment" />
    </fragment>
    <fragment
        android:id="@+id/myListFragment"
        android:name="com.example.atlmovie.ui.mylist.MyListFragment"
        android:label="fragment_my_list"
        tools:layout="@layout/fragment_my_list" />
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.atlmovie.ui.explore.ExploreFragment"
        android:label="ExploreFragment" />
    <fragment
        android:id="@+id/downloadFragment"
        android:name="com.example.atlmovie.ui.download.DownloadFragment"
        android:label="fragment_download"
        tools:layout="@layout/fragment_download" />
    <fragment
        android:id="@+id/notificationSettingsFragment"
        android:name="com.example.atlmovie.ui.profile.notification.NotificationSettingsFragment"
        android:label="fragment_notification_settings"
        tools:layout="@layout/fragment_notification_settings" />
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.atlmovie.ui.profile.editprofile.EditProfileFragment"
        android:label="EditProfileFragment" />
    <fragment
        android:id="@+id/privacyFragment"
        android:name="com.example.atlmovie.ui.profile.privacy.PrivacyFragment"
        android:label="fragment_privacy"
        tools:layout="@layout/fragment_privacy" />
    <fragment
        android:id="@+id/helpCenterFragment"
        android:name="com.example.atlmovie.ui.profile.helpcenter.HelpCenterFragment"
        android:label="fragment_help_center"
        tools:layout="@layout/fragment_help_center" />
    <fragment
        android:id="@+id/contactUsFragment"
        android:name="com.example.atlmovie.ui.profile.helpcenter.contactus.ContactUsFragment"
        android:label="fragment_contact_us"
        tools:layout="@layout/fragment_contact_us" />
    <fragment
        android:id="@+id/faqFragment"
        android:name="com.example.atlmovie.ui.profile.helpcenter.faq.FaqFragment"
        android:label="fragment_faq"
        tools:layout="@layout/fragment_faq" />
    <fragment
        android:id="@+id/subscribeToPremiumFragment"
        android:name="com.example.atlmovie.ui.profile.cards.SubscribeToPremiumFragment"
        android:label="fragment_subscribe_to_premium"
        tools:layout="@layout/fragment_subscribe_to_premium" >
        <action
            android:id="@+id/action_subscribeToPremiumFragment_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentMethodFragment"
        android:name="com.example.atlmovie.ui.profile.cards.PaymentMethodFragment"
        android:label="fragment_payment_method"
        tools:layout="@layout/fragment_payment_method" >
        <argument
            android:name="paymentargs"
            app:argType="string" />
        <action
            android:id="@+id/action_paymentMethodFragment_to_addNewFragment"
            app:destination="@id/addNewFragment" />
        <action
            android:id="@+id/action_paymentMethodFragment_to_reviewSummaryFragment"
            app:destination="@id/reviewSummaryFragment" />
    </fragment>
    <fragment
        android:id="@+id/addNewFragment"
        android:name="com.example.atlmovie.ui.profile.cards.AddNewFragment"
        android:label="fragment_add_new"
        tools:layout="@layout/fragment_add_new" />
    <fragment
        android:id="@+id/reviewSummaryFragment"
        android:name="com.example.atlmovie.ui.profile.cards.ReviewSummaryFragment"
        android:label="fragment_review_summary"
        tools:layout="@layout/fragment_review_summary">
        <argument
            android:name="cardNumber"
            app:argType="string" />
        <argument
            android:name="cardImage"
            app:argType="integer" />
        <argument
            android:name="paymentName"
            app:argType="string"
            android:defaultValue="" />
        <action
            android:id="@+id/action_reviewSummaryFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
</navigation>